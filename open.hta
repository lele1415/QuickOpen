<input type="button" onclick="runApplication('F:\ZC\tools\vbs\checklist\write_checklist.hta')" value="ChecklistTool" />
<input type="button" onclick="runApplication('F:\ZC\tools\vbs\apk_name\replaceStrInFileName_v1.3.hta')" value="FileNameTool" />
<br>
<input type="button" OnClick="self.close()" value="Exit" />

<Script Language="JavaScript" src="toolFun.js"></Script>
<Script Language="JavaScript" src="open.js"></Script>
<Script Language="VBScript" src="open.vbs"></Script>

</Body>
